<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
    
<hibernate-mapping>


<class 
    name="com.srb.hibernate.ClassInfoHibernate" 
    table="class_master"
>

    <id
        name="pkClassId"
        type="java.lang.Long"
        column="pk_class_id"
    >
    
        <generator class="increment" />
    </id>

    <property
        name="clas"
        type="java.lang.String"
        column="class_name"
        
    />
    <property
        name="classRoom"
        type="java.lang.String"
        column="room_number"
        
    />
     </class>
</hibernate-mapping>
    