<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
    
<hibernate-mapping>
<class  
    name="com.srb.hibernate.BookStockHibernate" 
    table="book_stock"
>

    <id
        name="pkBookStockId"
        type="java.lang.Long"
        column="pk_book_stock_id"
    >
    
        <generator class="increment" />
    </id>



         
    <property
        name="bookName"
        type="java.lang.String"
        column="book_name"
        
    />
    
        <property
        name="authorName"
        type="java.lang.String"
        column="author_name"
        
    />
    
        <property
        name="publisherName"
        type="java.lang.String"
        column="publisher_name"
        
    />
    
    <property
        name="supplierName"
        type="java.lang.String"
        column="supplier_name"
        
    />
    
     <property
        name="Status"
        type="java.lang.String"
        column="Status"
        
    />
    
 <!--      <property
        name="buyPrice"
        type="java.lang.Double"
        column="buy_price"
        
    /> -->
    <property
        name="quantity"
        type="java.lang.Long"
        column="quantity"
        
    />
    

    

     </class>
</hibernate-mapping>