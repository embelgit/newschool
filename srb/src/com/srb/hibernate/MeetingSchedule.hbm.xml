<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
    
<hibernate-mapping>
<class 
    name="com.srb.hibernate.MeetingScheduleHibernate" 
    table="schedule_meetings"
>
    <id
        name="pkScheduleMeetingsId"
        type="java.lang.Long"
        column="pk_schedule_meetings_id"
    >
        <generator class="increment" />
    </id>

    <property
        name="fkclassId"
        type="java.lang.Long"
        column="fk_class_id"
        
    />

    <property
        name="fkDivId"
        type="java.lang.Long"
        column="fk_div_id"
        
    />
    <property
        name="message"
        type="java.lang.String"
        column="message"
    />
    
     <property
        name="insertDate"
        type="java.sql.Date"
        column="insert_date"
        
    />
     </class>
</hibernate-mapping>
    