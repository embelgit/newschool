<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
    
<hibernate-mapping>


<class  
    name="com.srb.hibernate.ParentsComplaintHibernate" 
    table="parents_complaints"
>

    <id
        name="pkParentsComplaintId"
        type="java.lang.Long"
        column="pk_parents_complaint"
    >
    
        <generator class="increment" />
    </id>

    <property
        name="fkClassId"
        type="java.lang.Long"
        column="fk_class_id"
    />
    
    <property
        name="fkDivisionId"
        type="java.lang.Long"
        column="fk_division_id"
    />
   
    <property
        name="className"
        type="java.lang.String"
        column="class_name"
    />
    
    <property
        name="division"
        type="java.lang.String"
        column="division"
    />
    
      <property
        name="parentName"
        type="java.lang.String"
        column="parent_name"
    />
    
     <property
        name="firstName"
        type="java.lang.String"
        column="first_name"
    />
    
    <property
        name="middleName"
        type="java.lang.String"
        column="middle_name"
    />
    
    <property
        name="lastName"
        type="java.lang.String"
        column="last_name"
    />
    
     <property
        name="academicYear"
        type="java.lang.String"
        column="academic_year"
    />
    
    <property
        name="task"
        type="java.lang.String"
        column="task"
    />
    
     <property
        name="taskPic"
        column="image"
    />
    
    <property
        name="insertDate"
        type="java.sql.Date"
        column="insert_date"
    />
    
     </class>
</hibernate-mapping>