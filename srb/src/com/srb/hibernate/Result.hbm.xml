<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>

<class  
    name="com.srb.hibernate.ResultHibernate" 
    table="result_entry"
>

    <id
        name="pkResultId"
        type="java.lang.Long"
        column="pk_result_entry_id"
    >
    
        <generator class="increment" />
    </id>

    <property
        name="fkClassId"
        type="java.lang.Long"
        column="fk_class_id"
        
    />
    <property
        name="fkDivId"
        type="java.lang.Long"
        column="fk_division_id"
    />
       <property
        name="totalMarks"
        type="java.lang.Long"
        column="total_marks"
    />
       <property
        name="fkExamId"
        type="java.lang.Long"
        column="fk_exam_name_id"
    />
   
    <property
        name="fkSubjectId"
        type="java.lang.Long"
        column="fk_subject_id"
     />
    
    
     <property
        name="fkStudentId"
        type="java.lang.Long"
        column="fk_student_id"
     />
    
    <property
        name="examName"
        type="java.lang.String"
        column="exam_name"
    />
    
    <property
        name="className"
        type="java.lang.String"
        column="class_name"
    />
    <property
        name="divisionName"
        type="java.lang.String"
        column="division_name"
        
    />
    
    <property
        name="subjectName"
        type="java.lang.String"
        column="subject_name"
    />
    
        <property
        name="lastName"
        type="java.lang.String"
        column="last_name"
    />
    
        <property
        name="middleName"
        type="java.lang.String"
        column="middle_name"
    />
    
        <property
        name="firstName"
        type="java.lang.String"
        column="first_name"
    />
    
     <property
        name="passingMarks"
        type="java.lang.Long"
        column="passing_marks"
     />
     
      <property
        name="obtainedMarks"
        type="java.lang.Long"
        column="obtained_marks"
     />
    

    
     <property
        name="academicYear"
        type="java.lang.String"
        column="academic_year"
    />
    
          <property
        name="insertDate"
        type="java.sql.Date"
        column="insert_date"
     />
     </class>
</hibernate-mapping>